{
  "name": "api-pmb",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:pmk-itb/api-pmb.git",
  "author": "Samuel <samnap11@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "dotenv -e .env.development nodemon src/index.ts",
    "test": "dotenv -e .env.test -- jest --coverage",
    "build": "tsc -p tsconfig.json && cp -r ./data/kabupaten ./dist/data",
    "prepare": "husky install",
    "dev:push": "dotenv -e .env.development -- prisma db push --force-reset",
    "test:push": "dotenv -e .env.test -- prisma db push --force-reset",
    "dev:seed": "dotenv -e .env.development -- prisma db seed --preview-feature",
    "test:seed": "dotenv -e .env.test -- prisma db seed --preview-feature",
    "heroku-postbuild": "tsc -p tsconfig.json",
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "start": "node dist/src/index.js"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.5.7",
    "@types/jest": "^26.0.24",
    "@types/node": "^18.11.15",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "faker": "^5.5.3",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "jest-mock-extended": "^2.0.1",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "prisma": "^2.28.0",
    "superagent": "^6.1.0",
    "supertest": "^6.1.4",
    "ts-jest": "^27.0.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@prisma/client": "^2.29.1",
    "cors": "^2.8.5",
    "dotenv": "^9.0.1",
    "dotenv-cli": "^4.0.0",
    "express": "^4.17.1"
  }
}
